function apbd_fd($dt) {
	if (is_null($dt))
		return '';
	else
		return date('d-m-Y',strtotime($dt));
}

function apbd_fd_long($dt) {
	if (is_null($dt))
		return '';
	else
		return t(date('j F Y',strtotime($dt)));
}

