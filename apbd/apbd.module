function isSuperuser() {
	$user =  user_load($GLOBALS['user']->uid);
	if (in_array('superuser', $user->roles))
		return true;
	return (false || isAdministrator());	
}

function isUserSKPD() {
	$user =  user_load($GLOBALS['user']->uid);
	if (in_array('skpd', $user->roles))
		return true;
	return false;
}

function isUserPembantu() {
	$user =  user_load($GLOBALS['user']->uid);
	if (in_array('pembantu', $user->roles))
		return true;
	return false;
}

function isUserSeksi() {
	$user =  user_load($GLOBALS['user']->uid);
	if (in_array('seksi', $user->roles))
		return true;
	return false;
}

function isAdministrator() {
	$user =  user_load($GLOBALS['user']->uid);
	if (user_access('Administer Nodes', $user))
		return true;
	return false;		
}

