function apbd_getnospj_gu($kodeuk) {
	

	$query = db_select('bendahara' . $kodeuk, 'd')->extend('TableSort');
	$query->fields('d', array('spjno'));
	$query->condition('d.kodeuk', $kodeuk, '=');

	$or = db_or();
	$or->condition('d.jenis', 'gu-spj', '=');
	$or->condition('d.jenis', 'gu-spj', '=');
	$query->condition($or);
	
	$query->orderBy('d.spjno', 'DESC');
	$query->range(0, 1);
	//dpq($query);
	
	$v = 0;
	# execute the query
	$results = $query->execute();
	foreach ($results as $data) {
		//drupal_set_message($data->sp2dno);
		//drupal_set_message(substr($data->sp2dno,0,5));
		$v = (int) substr($data->spjno,0,5);
	}		
	$v++;
	$s = '00000' . $v;
	$s = substr($s, strlen($s)-5);
	
	$s .= '-' . $kodeuk;

	return $s;

} 

function apbd_date_convert_db2form($tanggal) {
	$arr_tgl = explode("-", $tanggal);	
	return  mktime(0, 0, 0, $arr_tgl[1], $arr_tgl[2], $arr_tgl[0]);
}

function apbd_date_convert_form2db($tanggal) {
	//sprintf("%02d", $num)
	$hari = sprintf("%02d", $tanggal['day']);
	$bulan = sprintf("%02d", $tanggal['month']);
	return  $tanggal['year'] . '-' . $bulan . '-' . $hari;
}

function apbd_date_create_currdate_form() {
	return  mktime(0, 0, 0, date('m'), date('d'), apbd_tahun());
}

function apbd_date_create_dateone_form() {
	return  mktime(0, 0, 0, date('m'), 1, apbd_tahun());
}

function apbd_readrealisasikegiatan($kodekeg, $tanggal) {
	$cair = 0;
	
	$kodeuk = substr($kodekeg, 4, 2);
	
	//SPJ
	$query = db_select('bendahara' . $kodeuk, 'b');
	$query->addExpression('SUM(b.total)', 'total');

	$or = db_or();
	$or->condition('b.jenis', 'gaji', '=');
	$or->condition('b.jenis', 'ls', '=');
	$or->condition('b.jenis', 'gu-spj', '=');
	$or->condition('b.jenis', 'tu-spj', '=');
	$query->condition($or);
	
	$query->condition('b.kodekeg', $kodekeg, '=');
	//$query->condition('b.tanggal', $tanggal, '<=');
	$res_spj = $query->execute();
	foreach ($res_spj as $data_spj) {
		$cair = $data_spj->total;
	}

	//RETUR
	$query = db_select('bendahara' . $kodeuk, 'b');
	$query->addExpression('SUM(b.total)', 'total');

	$query->condition('b.kodekeg', $kodekeg, '=');
	$query->condition('b.jenis', 'ret-spj', '=');
	//$query->condition('b.tanggal', $tanggal, '<=');
	$res_spj = $query->execute();
	foreach ($res_spj as $data_spj) {
		$cair -= $data_spj->total;
	}	
	
	return $cair;
}

function apbd_readrealisasikegiatan_rekening($kodekeg, $kodero, $tanggal) {
	$cair = 0;
	
	//SPJ
	$kodeuk = substr($kodekeg, 4, 2);
	$query = db_select('bendahara' . $kodeuk, 'b');
	$query->innerJoin('bendaharaitem' . $kodeuk, 'bi', 'b.bendid=bi.bendid');
	$query->addExpression('SUM(bi.jumlah)', 'total');

	$or = db_or();
	$or->condition('b.jenis', 'gaji', '=');
	$or->condition('b.jenis', 'ls', '=');
	$or->condition('b.jenis', 'gu-spj', '=');
	$or->condition('b.jenis', 'tu-spj', '=');
	$or->condition('b.jenis', 'pindahbuku', '=');
	$query->condition($or);
	
	$query->condition('b.kodekeg', $kodekeg, '=');
	//$query->condition('b.tanggal', $tanggal, '<=');
	$query->condition('bi.kodero', $kodero, '=');
	$res_spj = $query->execute();
	foreach ($res_spj as $data_spj) {
		$cair = $data_spj->total;
	}

	//RETUR
	if ($cair>0) {
		$query = db_select('bendahara' . $kodeuk, 'b');
		$query->innerJoin('bendaharaitem' . $kodeuk, 'bi', 'b.bendid=bi.bendid');
		$query->addExpression('SUM(bi.jumlah)', 'total');

		$query->condition('b.kodekeg', $kodekeg, '=');
		$query->condition('b.jenis', 'ret-spj', '=');
		//$query->condition('b.tanggal', $tanggal, '<=');
		$query->condition('bi.kodero', $kodero, '=');
		$res_spj = $query->execute();
		foreach ($res_spj as $data_spj) {
			if ($data_spj->total>0)
				$cair -= $data_spj->total;
			else
				$cair += $data_spj->total;
		}	
	}
	return $cair;
}

function dateapi_convert_timestamp_to_datetime($timestamp) {

	$date = new DateTime($timestamp);
	//date_timezone_set($date, timezone_open(drupal_get_user_timezone()));
	return date_format($date, 'Y-m-d');
}

function dateapi_convert_datetime_to_timestamp($datetime) {
	
	$date = date_create_from_format('Y-m-d',$datetime,timezone_open(drupal_get_user_timezone()));
	return date_timestamp_get($date);
}

